{
  "name": "alignment-viewer-2",
  "homepage": "https://fast.alignmentviewer.org",
  "version": "2.0.0-beta-2",
  "private": false,
  "author": "Nicholas Gauthier",
  "main": "dist/alignment-viewer-2.cjs.js",
  "module": "dist/alignment-viewer-2.es.js",
  "types": "dist/js/index.d.ts",
  "contributors": [
    "Yang Su",
    "Andrew Diamantoukos",
    "Chris Sander"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sanderlab/AlignmentViewer2.0"
  },
  "license": "MIT",
  "dependencies": {
    "@react-hook/size": "2.1.2",
    "@reduxjs/toolkit": "2.0.1",
    "pixi.js": "7.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "9.0.4",
    "react-select": "5.8.0",
    "react-tooltip": "5.25.1",
    "sass": "1.69.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@types/enzyme": "3.10.16",
    "@types/enzyme-adapter-react-16": "1.0.9",
    "@types/jest": "29.5.10",
    "@types/react": "18.2.39",
    "@types/react-redux": "7.1.32",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.4.4",
    "gh-pages": "^2.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest-canvas-mock": "2.5.2",
    "jest-environment-jsdom-fifteen": "1.0.2",
    "jest-fetch-mock": "3.0.3",
    "jest-transform-css": "6.0.1",
    "jest-webgl-canvas-mock": "2.5.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.2.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "cover": "jest --coverage --watchAll=false",
    "build-and-deploy": "npm run build && npm run deploy",
    "deploy": "gh-pages -d dist"
  },
  "files": [
    "dist"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/**/__tests__/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "coverageReporters": [
      "lcov",
      "json",
      "text"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|cssmodule)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "\\.tsx?$": "ts-jest"
    }
  }
}
